<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.think.dao.UserMapper">
    <insert id="insert" parameterType="com.think.entity.User" >
        insert into
        user(user_name,age,phone,create_date,update_date)
        values(#{userName},#{userAge},#{phone},#{createDate},#{updateDate})
    </insert>
</mapper>